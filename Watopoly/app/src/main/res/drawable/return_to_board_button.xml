<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <solid android:color="#FFFFFF"/>
            <size
                android:width="50dp"
                android:height="50dp"/>
            <stroke
                android:width="1dp"
                android:color="#000000"/>
        </shape>
    </item>
    <item android:height="25dp" android:top="13dp"
        android:left="7dp" android:right="7dp">
        <shape android:shape="rectangle">
            <stroke
                android:width="0.5dp"
                android:color="#000000"/>
        </shape>
    </item>
    <item android:left="10dp" android:right="10dp"
        android:height="20dp" android:top="15.6dp">
        <shape android:shape="rectangle">
            <stroke
                android:width="0.5dp"
                android:color="#000000"/>
        </shape>
    </item>
</layer-list>